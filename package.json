{
  "name": "react-redux-universal-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for a universal web app using react, redux",
  "main": "app.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "postinstall": "npm run build",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "lint": "eslint src/*",
    "test": "mocha --compilers js:babel-register --require test/setup.js test/**/*-test.js",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "start": {
      "command": "node .",
      "env": {
        "NODE_ENV": "production",
        "HOSTNAME": "https://react-universal-starter-kit.herokuapp.com"
      }
    },
    "build": {
      "command": "webpack --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "HOSTNAME": "https://react-universal-starter-kit.herokuapp.com"
      }
    },
    "dev": {
      "command": "node .",
      "env": {
        "NODE_ENV": "development",
        "HOSTNAME": "http://localhost",
        "PORT": 3000
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suranartnc/react-redux-universal-starter-kit.git"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "isomorphic",
    "ES2015",
    "webpack"
  ],
  "author": "Suranart Niamcome",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suranartnc/react-redux-universal-starter-kit/issues"
  },
  "homepage": "https://github.com/suranartnc/react-redux-universal-starter-kit#readme",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "cssnano": "^3.5.2",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.2",
    "json-server": "^0.8.8",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "pure-render-decorator": "^1.0.0",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-addons-perf": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.1",
    "react-facebook-login": "^2.1.3",
    "react-helmet": "^2.3.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-form": "^4.2.2",
    "scroll-behavior": "^0.3.3",
    "style-loader": "^0.13.0",
    "updeep": "^0.14.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
